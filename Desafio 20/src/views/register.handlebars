<div class="main">
  <div class=" col-5 ">
    <div class="">
      <h2 class="formTitle d-flex justify-content-center">Registro</h2>

      <form id="registerForm">
        <label>Correo Electrónico</label>
        <input type="text" class="form-control" name="email" required><br>
        <label>Nombre:</label>
        <input type="text" class="form-control" name="name" required><br>
        <label>Apellido:</label>
        <input type="text" class="form-control" name="last_name" required><br>
        <label>Edad:</label>
        <input type="number" class="form-control" name="age" required><br>
        <label>Nombre de Usuario:</label>
        <input type="text" class="form-control" name="nickname" required><br>
        <label>Contraseña:</label>
        <input type="password" class="form-control" name="password" required><br>
        <label>Avatar URL:</label>
        <input type="text" class="form-control" name="avatar" required><br>
        <input type="submit" class="btn btn-info" value="Registrarse"> 
        <input type="submit" class="btn btn-info" onclick="location.href='/login';" value="Ir a ingresar"> 

      </form>
            
    </div>    
        
  </div>
    

       
</div>


